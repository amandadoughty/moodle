{{!
    @template block_culupcoming_events/footer

    CUL footer view.

    The purpose of this template is to render the footer.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Example context (json):
    {
    }
}}

{{#footer}}
<div class="calendarurl-btn">
	<a data-toggle="tooltip" title="{{#str}} gotocalendar, calendar {{/str}}" href="{{{calendarurl}}}">
		{{#pix }} i/calendar, core {{/pix }}
        <span class="accesshide">{{#str}} gotocalendar, calendar {{/str}}</span>
	</a>
</div>
{{#manageentries}}
<div class="addevent-btn">
	<a data-toggle="tooltip" title="{{#str}} newevent, calendar {{/str}}" href="{{{addeventurl}}}">
		{{#pix }} t/add, core {{/pix }}
        <span class="accesshide">{{#str}} newevent, calendar {{/str}}</span>
	</a>
</div>
{{/manageentries}}
{{/footer}}